<?xml version="1.0" encoding="ISO-8859-1" ?>
<?xml-stylesheet type="text/xsl" href="xsl/mdb2_changeset.xsl"?>
<instructionset>
  <name>openads</name>
  <version>540</version>
  <comments>Active to status, plus other</comments>
  <constructive>
    <changeset>
      <name>openads</name>
      <version>540</version>
      <change>
        <table>
          <name>banners</name>
          <add>
            <field>
              <name>an_banner_id</name>
              <type>openads_int</type>
              <length>11</length>
              <notnull></notnull>
              <default></default>
              <was>an_banner_id</was>
            </field>
              <field>
                <name>as_banner_id</name>
                <type>openads_int</type>
                <length>11</length>
                <notnull></notnull>
                <default></default>
                <was>as_banner_id</was>
              </field>
                <field>
                  <name>status</name>
                  <type>openads_int</type>
                  <length>11</length>
                  <notnull>1</notnull>
                  <default>0</default>
                  <was>status</was>
                </field>
              </add>
            </table>
              <table>
                <name>campaigns</name>
                <add>
                  <field>
                    <name>an_campaign_id</name>
                    <type>openads_int</type>
                    <length>11</length>
                    <notnull></notnull>
                    <default></default>
                    <was>an_campaign_id</was>
                  </field>
                    <field>
                      <name>as_campaign_id</name>
                      <type>openads_int</type>
                      <length>11</length>
                      <notnull></notnull>
                      <default></default>
                      <was>as_campaign_id</was>
                    </field>
                      <field>
                        <name>status</name>
                        <type>openads_int</type>
                        <length>11</length>
                        <notnull>1</notnull>
                        <default>0</default>
                        <was>status</was>
                      </field>
                        <field>
                          <name>an_status</name>
                          <type>openads_int</type>
                          <length>11</length>
                          <notnull>1</notnull>
                          <default>0</default>
                          <was>an_status</was>
                        </field>
                      </add>
                    </table>
                  </change>
                </changeset>
              </constructive>
              <destructive>
                <changeset>
                  <name>openads</name>
                  <version>540</version>
                  <change>
                    <table>
                      <name>banners</name>
                    <remove>
                      <field>
                        <name>active</name>
                      </field>
                        <field>
                          <name>oac_banner_id</name>
                        </field>
                      </remove>
                    </table>
                      <table>
                        <name>campaigns</name>
                      <remove>
                        <field>
                          <name>active</name>
                        </field>
                          <field>
                            <name>oac_campaign_id</name>
                          </field>
                        </remove>
                      </table>
                    </change>
                  </changeset>
                </destructive>
              </instructionset>
